{
  "name": "noflo-test",
  "description": "NoFlo component testing library",
  "version": "0.0.5",
  "dependencies": {
    "vows": "0.7.x",
    "coffee-script": "1.6.x",
    "underscore": "1.4.x",
    "noflo": "~0.4.0"
  },
  "devDependencies": {
    "coffeelint": "*"
  },
  "main": "./index.js",
  "bin": {
    "noflo-test": "./bin/noflo-test"
  },
  "scripts":    {
    "pretest": "./node_modules/.bin/coffeelint -r lib",
    "test": "./bin/noflo-test --spec test/*.coffee"
  }
}
